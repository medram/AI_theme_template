							<div class="my-5 d-flex flex-wrap flex-column flex-md-row">
								<a class="btn btn-lg btn-primary mb-3 mx-2" href="/blog" target="_blank"><i class="bi bi-arrow-right-circle"></i> Learn More</a>
								<a class="btn btn-lg btn-outline-secondary mb-3 mx-2" href="/#contact">Get in touch</a>
							</div>
						</div>
						<div class="col-md-6 text-center animate__animated animate__fadeInRight">
							<img class="mw-100" src="assets/imgs/posters/2.jpg" alt="#">
						</div>
					</div>
				</div>
			</section>

		</div>

		<!-- stats section -->
		<section class="animate__animated" id="stats">
			<div class="stats">
